@{
    ViewBag.Title = "Index";
}
<div>
    <h4 class="titulo-tesoreria alert alert-info"><strong>Analisis Rules</strong></h4>
    <div class="row contenido-general">
        <div class="col-lg-6">
            <from lass="form-inline">
                <label for="lblBuscar">Identificación:</label>
                <input id="txtBusqueda" type="text" name="msg">
                <button type="button" class="btn btn-primary" Id="btnBuscarRules">Buscar</button>
                <button type="button" class="btn btn-primary btn-primary-eliminar" Id="btnCerrarRules">Cerrar Tabla</button>
            </from>
        </div>
        <div class="col-lg-6"></div>
    </div>
    <div id="contenido-tabla-rules" class="row contenido-general" style="display:none">
        <div class="col-lg-12">
            <table id="TableRules" class="container table table-bordered table-hover dataTable">
                <thead>
                    <tr class="info">
                        <th>
                            IdSolicitud
                        </th>
                        <th>
                            Identificación
                        </th>
                        <th>
                            MotivoRechazo
                        </th>
                        <th>
                            DescripcionMotivoRechazo
                        </th>
                        <th>
                            FechaIngreso
                        </th>
                        <th>
                            Descripción
                        </th>
                    </tr>
                </thead>
                <tbody class="tbody" id="tblrules"></tbody>
            </table>
        </div>
    </div>
</div>
<script src="~/Scripts/Proyecto/AnalisisRules/AnalisisRules.js"></script>