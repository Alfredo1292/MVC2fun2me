@{
	Layout = null;
}
<h4 id="mensajeAccion" class="titulo-tesoreria accordion-toggle alert" style="display:none"></h4>
<div>
	<div class="form-group">

	</div>
	<table ID="UserTable" class="container table table-bordered table-hover no-footer dataTable">
		<thead>
			<tr class="info">
				<th>
					Agente
				</th>
				<th>
					Nombre
				</th>
				<th>
					Correo
				</th>
				<th>
					Estado
				</th>
				<th>
					ROL
				</th>
				<th id="thUserTableAccion" width="300px;">
					Accion
				</th>
			</tr>
		</thead>
		<tbody class="tbody"></tbody>
	</table>
</div>
<style type="text/css">
	.thUserTableAccion {
		width: 300px !important;
	}
</style>
<div class="modal fade" Id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
                <button type="button" class="close" data-dismiss="modal">x</button>
                <div class="modal-header titulo-modal">
                    <p id="idEditarAgente" class="alert alert-info"><strong></strong></p>
                </div>
			<div class="modal-body modal-editar-agente">
                <form>
                    <div class="row">
                        <div class="form-group col-lg-6">
                            <label for="cod_agente">CodigoAgente</label>
                            <input type="text" class="form-control control-editar-usuario" id="cod_agente" placeholder="CodigoAgente" disabled="disabled" />
                        </div>
                        <div class="form-group col-lg-6">
                            <label for="nombre">Nombre</label>
                            <input type="text" class="form-control control-editar-usuario" id="nombre" placeholder="nombre" />
                        </div>
                    </div>                   
                    <div> 
                        <div class="form-group col-lg-6">
                            <label for="correo">Correo</label>
                            <input type="text" class="form-control control-editar-usuario" id="correo" placeholder="correo" />
                        </div>
                        <div class="form-group col-lg-6">
                            <label for="estado">Estado</label>
                            <input type="text" class="form-control control-editar-usuario" id="estado" placeholder="estado" />
                        </div>
                    </div>
                    <div>
                        <div class="form-group col-lg-6">
                            <label for="ROLID">Rol</label>
                            <select id="ddlRolId" class="form-control control-editar-usuario"></select>
                            @*<input type="text" class="form-control" id="ROLID" placeholder="ROLID" />*@
                        </div>
                        <div class="form-group col-lg-6">
                            <label for="TipoCola">Asignacion</label>
                            <select id="ddAsignacion" class="form-control control-editar-usuario"></select>
                        </div>
                    </div>
                    <div>
                        <div class="form-group col-lg-6">
                            <label for="AgenciaExt">Agencia</label>
                            <select id="ddlAgencia" class="form-control control-editar-usuario"></select>
                        </div>
                    </div>
                </form>
			</div>
			<div class="modal-footer footer-editar-agente">
				<button type="button" class="btn btn-primary" Id="btnUpdate" style="display:none;" onclick="Update();">Actualizar</button>
				<button type="button" class="btn btn-default" data-dismiss="modal" onclick="clearTextBox();">Cerrar</button>
			</div>
		</div>
	</div>
</div>
<div class="modal fade" Id="myModalAsigUsu" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
                <button type="button" class="close" data-dismiss="modal" onclick="clearMODAL2();">x</button>
                <div class="modal-header titulo-modal">
                    <p id="idEditarAgente" class="alert alert-success"><strong>Asignar Pantallas a Usuarios</strong></p>
                </div>
			<div class="modal-body asignar-menu">
				<form>
					<div class="row">
						<div class="col-sm-6 subtitulos-asignar-menu">
							<p class="">Codigo Agente: <label id="lblCOD_AGENTEtex" style="font-size:12px" class="text-muted"></label></p>
						</div>
						<div class="col-sm-6 subtitulos-asignar-menu">
							<p class="">Nombre: <label id="lblnombre" style="font-size:12px" class="text-muted"></label></p>
						</div>
					</div>

                    <div class="row anyClass">
                        <div class="col-sm-3 " style="width:15px;">

                        </div>
                        <div class="col-sm-5 ">
                            <div class="list-group lista-menu" id="listDesAsignado">
                                <a href="#" class="list-group-item head-list" value="1">Sin Asignar <input title="toggle all" type="checkbox" class="all pull-right"></a>
                            </div>
                        </div>
                        <div class="col-md-1 v-center">
                            <a class="btn btn-asignar-menu add head-list"><i class="glyphicon glyphicon-chevron-right"></i></a>
                            <a class="btn btn-asignar-menu remove head-list"><i class="glyphicon glyphicon-chevron-left"></i></a>

                        </div>
                        <div class="col-sm-5">
                            <div class="list-group lista-menu" id="listAsignado">
                                <a href="#" class="list-group-item head-list">Asignado <input title="toggle all" type="checkbox" class="all pull-right"></a>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary btn-primary-asignar" Id="btnActualiza" style="display:none;" data-dismiss="modal" onclick="Actualizar();clearMODAL2();">Actualizar</button>
                <button type="button" class="btn btn-primary btn-warning" data-dismiss="modal" onclick="clearMODAL2();">Cerrar</button>
            </div>
        </div>
    </div>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>

<script type="text/javascript">
            @if (!User.Identity.IsAuthenticated)
            {

                System.Web.Security.FormsAuthentication.SignOut();
                System.Web.Security.FormsAuthentication.RedirectToLoginPage();
                Response.Redirect(FormsAuthentication.LoginUrl, true);
            }

</script>