@{
    Layout = null;
}
<div class="table" >

    <table ID="HistoPromPagoTable" class="container table table-bordered table-hover no-footer dataTable">
        <thead>
            <tr class="info">
                <th >
                    Crédito
                </th>
                <th>
                    Fecha Cobro
                </th>
                <th>
                    Fecha Aplicación
                </th>
                <th>
                    Total
                </th>
                <th>
                    Referencia
                </th>
            </tr>
        </thead>
        <tbody class="tbody HistoPromPago"></tbody>
    </table>
</div>
<div class="table">
    <table ID="PlanPagoTable" class="container table table-bordered table-hover no-footer dataTable">
        <thead>
			<tr class="info">
				<th>
					Crédito
				</th>
				<th>
					Cuota
				</th>
				<th>
					Fecha Vencimiento
				</th>
				<th>
					Monto Capital
				</th>
				<th>
					Monto Interés
				</th>
				<th>
					Monto Originación
				</th>
				<th>
					Monto Intetrés Mora
				</th>
				<th>
					Monto Cobrado
				</th>
				<th>
					Monto x Mora
				</th>
			</tr>
        </thead>
        <tbody class="tbody planPagos"></tbody>
    </table>
</div>

<script src="~/Scripts/Proyecto/ColaCobros.js"></script>
<script type="text/javascript">
            @if (!User.Identity.IsAuthenticated)
            {

                System.Web.Security.FormsAuthentication.SignOut();
                System.Web.Security.FormsAuthentication.RedirectToLoginPage();
                Response.Redirect(FormsAuthentication.LoginUrl, true);
            }

</script>