                <p id="lblTiempoMinuto">00</p>
                <p id="lblDosPuntos">:</p>
                <p id="lblTiempoSegundo">00</p>
            </div>
        </div>
    </div>
    <div class="contenido-general row">
        <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="table-responsive">
                <form class="form-inline" onSubmit="return false;">
                    <div class="btn-group btn-group-s" role="group">
                    </div>
                    <button type="button" id="btnFiltrar" onclick="MostrarFiltros();" required class="btn btn-primary btn-group-s"><span class="fas fa-angle-double-down"></span><span class="tooltiptexGeneral">Buscar Por Nombre</span></button>
                    <input id="txtBusqueda" type="text" class="form-control input-sm" name="msg">
                    <button type="button" id="btnBuscar" onclick="buscarSolicitudManual();" required class="btn btn-primary btn-group-s"><span class="glyphicon glyphicon-search"></span> Buscar</button>
                </form>
            </div>
        </div>
        <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="table-responsive">
                <form class="form-inline">
                    <div class="btn-group btn-group-s" role="group"></div>
                    <button type="button" id="btnComentarioReprog" onclick="mostrarModalComentarioRepro();" required class="btn btn-primary btn-primary-editar"><span class="glyphicon glyphicon-calendar"></span> Reprog</button>
                </form>
            </div>
        </div>
        <div class="col-lg-2 col-md-6 col-sm-12">
            <div class="table-responsive">
                <form class="form-inline">
                    <div role="group">
                        <button type="button" id="button_play" onclick="buttonPlayPress();" class="btn btn-primary btn-primary-asignar" style="display:none">
                            <i id="button_play_i" class="fa fa-play"></i>
                            @*<span class="glyphicon glyphicon-play-circle"></span>*@
                            Iniciar
                        </button>
                        <button type="button" id="button_stop" onclick="buttonStopPress();" class="btn btn-primary btn-primary-eliminar" style="display:none">
                            <i id="button_stop_i" class="fa fa-stop"></i>
                            @*<span class="glyphicon glyphicon-step-forward"></span>*@
                            Detener
                        </button>
                        <button type="button" id="button_fw" onclick="buttonForwardPress();" class="btn btn-warning" style="display:none;">
                            <i id="button_fw_i" class="fa fa-forward"></i>
                            @*<span class="glyphicon glyphicon-step-forward"></span>*@
                            Sguiente
                        </button>
                        <button type="button" id="button_reiniciarCola" onclick="reiniciarCola();" class="btn btn-primary btn-primary-asignar" style="display:none">
                            <i id="button_reiniciar" class="fa fa-play"></i>
                            @*<span class="glyphicon glyphicon-step-forward"></span>*@
                            Continuar
                        </button>
                    </div>
                </form>
            </div>
        </div>
